# Environment variables for Splitwise Super Saiyan backend
# Copy this file to .env and fill in your actual values

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_KEY=your_supabase_anon_key

# Google OAuth Configuration (for client-side verification)
GOOGLE_CLIENT_ID=your_google_oauth_client_id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your_google_oauth_client_secret

# Gemini AI Configuration (for receipt processing)
GEMINI_API_KEY=your_gemini_api_key

# JWT Configuration
JWT_SECRET_KEY=your_secret_key_for_jwt_signing

# Environment
ENVIRONMENT=development